{
  "name": "n8n-nodes-sinergiacrm",
  "version": "1.1.0",
  "description": "A generic n8n node to operate with any SinergiaCRM (SuiteCRM 7.x+) module via the official JSON API.",
  "type": "module",
  "main": "dist/nodes/SinergiaCRM/SinergiaCRM.node.js",
  "scripts": {
    "build": "rimraf dist && tsc && cp nodes/SinergiaCRM/sinergiacrm.svg dist/nodes/SinergiaCRM/",
    "lint": "eslint ."
  },
  "keywords": [
    "n8n",
    "sinergiacrm",
    "suitecrm",
    "custom-node",
    "n8n-community-node",
    "n8n-community-nodes",
    "n8n-custom-node",
    "n8n-custom-nodes",
    "n8n-node",
    "n8n-nodes",
    "n8n-node-package",
    "n8n-community-node-package"
  ],
  "author": "Javier Qu√≠lez <javier.quilez@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tecnologia-Solidaria/n8n-nodes-sinergiacrm"
  },
  "bugs": {
    "url": "https://github.com/Tecnologia-Solidaria/n8n-nodes-sinergiacrm/issues"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/SinergiaCRM/SinergiaCRM.node.js"
    ],
    "credentials": [
      "dist/credentials/SinergiaCRMCredentials.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "eslint": "8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
